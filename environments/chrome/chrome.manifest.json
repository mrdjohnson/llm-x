{
  "manifest_version": 3,
  "name": "LLM-X",
  "version": "1.0.3",
  "action": {},
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": ["sidePanel", "declarativeNetRequest"],
  "host_permissions": [
    "http://*:11434/*",
    "http://*:1234/*",
    "http://*:7860/*",
    "https://*:11434/*",
    "https://*:1234/*",
    "https://*:7860/*"
  ],
  "icons": {
    "16": "logo-16x16.png",
    "32": "logo-32x32.png",
    "48": "logo-48x48.png",
    "128": "logo-128x128.png"
  },
  "background": {
    "service_worker": "environments/chrome/chrome.background.ts"
  }
}
